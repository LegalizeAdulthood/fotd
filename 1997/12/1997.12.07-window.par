window             { ; Fractal of the day, 07-12-97
                     ; 14 min on a 486DX4, 640x480
  reset=1960 type=formula 
  formulaname=MandelbrotMix passes=t
  center-mag=0.310749/-0.445211/6.545123/1/-87.5
  params=1/2/0.154/3/-7.325/0.05 float=y maxiter=6000
  bailout=25 inside=253 symmetry=none periodicity=10
  ranges=0/-1/50/65/80/100/130/170/220/280/350/430/1000/6000
  colors=000G80YOFQb0Gh00k00n00q0Lt0Qx0_z0dzhizrnzz000<237>0\
  000Zh000000
  }

frm:MandelbrotMix {; Jim Muth
z=p3, c=pixel,
a=real(p1), b=imag(p1), d=real(p2), f=imag(p2):
z=(a*(z^b))+(d*(z^f))+c,
|z| <= 100
}
